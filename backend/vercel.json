{
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"],
        "headers": {
            "Access-Control-Allow-Origin": "*"
        },
        "status": 200
      }
    ],
    "functions": {
        "api/index.js": {
        "runtime": "nodejs18.x"
        }
  }
  }
  